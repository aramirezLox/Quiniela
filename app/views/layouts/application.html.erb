<!DOCTYPE html>
<html>
  <head>
    <title>Quiniela</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <%= button_to "Sign out", destroy_user_session_path, method: :delete %>
    <%= button_to 'Sign In', user_session_path, method: :get %>
    <%= button_to "Predictions",  predictions_path, method: :get %>
    <%= button_to "Club",  clubs_path, method: :get %>
    <%= button_to "Score",  scores_path, method: :get %>
    <%= button_to "Match",  matches_path, method: :get %>
    
    <%= yield %>
  </body>
</html>
